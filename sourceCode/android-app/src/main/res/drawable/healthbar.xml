<?xml version="1.0"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android"
  >
  <item
    android:id="@android:id/background"
    >
    <shape>
      <corners
        android:radius="2dp"
        />
      <solid
        android:color="@android:color/transparent"
        />
      <stroke
        android:color="#33aaaaaa"
        android:width="1dp"
        />
    </shape>
  </item>
  <item
    android:bottom="1dp"
    android:id="@android:id/progress"
    android:left="1dp"
    android:right="1dp"
    android:top="1dp"
    >
  <clip
    android:clipOrientation="horizontal"
    android:gravity="start"
    >
  <shape>
    <corners
      android:radius="2dp"
      />
    <gradient
      android:endColor="#24e824"
      android:startColor="#24e824"
      />
  </shape>
</clip>
</item>
</layer-list>
